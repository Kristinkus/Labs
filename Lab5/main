int main() {
    int size;
    printf("Введите размер стека: ");
    scanf("%d", &size);

    Stack* stack = createStack(size);

    printf("Введите элементы стека:\n");
    for (int i = 0; i < size; i++) {
        int value;
        scanf("%d", &value);
        push(stack, value);
    }

    int sum = sumEvenNumbers(stack);
    printf("Сумма чётных чисел: %d\n", sum);

    freeStack(stack);
    return 0;
}
